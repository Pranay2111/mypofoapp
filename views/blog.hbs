<div class="container">
  <div class="nav-scroller py-1 mb-2">
    <nav class="nav d-flex justify-content-between">
      <a class="p-2 text-muted" href="#">HTML</a>
      <a class="p-2 text-muted" href="#">CSS</a>
      <a class="p-2 text-muted" href="#">Javascript</a>
      <a class="p-2 text-muted" href="#">Node.Js</a>
      <a class="p-2 text-muted" href="#">Express</a>
      <a class="p-2 text-muted" href="#">MongoDB</a>
      <a class="p-2 text-muted" href="#">REsT API</a>
      <a class="p-2 text-muted" href="#">Angular</a>
      <a class="p-2 text-muted" href="#">Bootstrap</a>

    </nav>
  </div>

  <div class="jumbotron p-3 p-md-5 text-white rounded bg-dark">
    <div class="col-md-6 px-0">
      <h1 class="display-4 font-italic">HTML</h1>
      <p class="lead my-3">Multiple lines of text that form the lede, informing new readers quickly and efficiently
        about what’s most interesting in this post’s contents.</p>
      <p class="lead mb-0"><a href="#" class="text-white font-weight-bold">Continue reading...</a></p>
    </div>

  </div>
  {{#each blogs}}
  <div class="row mb-2">
    <div class="col-md-12">
      <div class="card flex-md-row mb-4 shadow-sm h-md-250">
        <div class="card-body d-flex flex-column align-items-start">
          <strong class="d-inline-block mb-2 text-primary">{{this.tag.name}}</strong>
          <h3 class="mb-0">
            <a class="text-dark" href="#">{{this.name}}</a>
          </h3>
          <div class="mb-1 text-muted">Nov 12</div>
          <p class="card-text mb-auto">{{this.description}}</p>
          <a href="#">Continue reading</a>
        </div>
        <img class='card-img-right flex-auto' width="200px" height="250px" src="/image/image1.jpg" alt="no image" {{this.image}}>
      </div>
    </div>
   {{/each}}
    

  </div>
</div>